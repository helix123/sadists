@article{dykstra_chibars1991,
 jstor_articletype = {research-article},
 title = {Asymptotic Normality for Chi-Bar-Square Distributions},
 author = {Dykstra, Richard},
 journal = {The Canadian Journal of Statistics / La Revue Canadienne de Statistique},
 jstor_issuetitle = {},
 volume = {19},
 number = {3},
 jstor_formatteddate = {Sep., 1991},
 pages = {pp. 297-306},
 url = {http://www.jstor.org/stable/3315395},
 ISSN = {03195724},
 abstract = {Chi-bar-square distributions, which are mixtures of chi-square distributions, mixed over their degrees of freedom, often occur when testing hypotheses that involve inequality constraints. Here, necessary and sufficient conditions on the mixing or weighting distribution are found to ensure asymptotic normality of the corresponding chi-bar-square distribution. Essentially, asymptotic normality occurs for the chi-bar-square distribution if either the ratio of the mean to the variance of the mixing distribution goes to infinity, or the weighting distribution itself is asymptotically normal. Other than a combination of these two phenomena, this is also the only way for asymptotic normality to hold. Several examples of pertinent chi-bar-square distributions are shown to be asymptotically normal by the results in this paper. /// Les lois dites "khi-deux pondérées" sont des mélanges aléatoires de lois khi-deux, le mélange étant par rapport aux degrés de liberté. Elles apparaissent fréquemment dans le contexte de tests d'hypothèses comprenant des contraintes sous forme d'inégalités. On donne des conditions nécessaires et suffisantes, relativement à la loi du mélange, pour assurer la normalité asymptotique de la loi "khi-deux pondérée" résultante. On a cette normalité asymptotique si le rapport entre la moyenne et la variance de la loi du mélange tend vers l'infini, ou si la loi du mélange est elle-même asymptotiquement normale. Ces deux cas, ou une combination de ceux-ci, sont les seules situations pouvant assurer la normalité asymptotique. Plusieurs exemples de lois "khi-deux pondérées" sont analysés à l'aide des résultats obtenus.},
 language = {English},
 year = {1991},
 publisher = {Statistical Society of Canada},
 copyright = {Copyright © 1991 Statistical Society of Canada},
}

